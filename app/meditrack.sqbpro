CREATE TABLE IF NOT EXISTS pengguna (
    idPengguna    INTEGER PRIMARY KEY AUTOINCREMENT,
    nama          TEXT    NOT NULL,
    email         TEXT    NOT NULL UNIQUE,
    password      TEXT    NOT NULL,
    tanggalLahir  TEXT,  
    jenisKelamin  TEXT    NOT NULL CHECK(jenisKelamin IN ('Pria','Wanita','Lainnya')),
