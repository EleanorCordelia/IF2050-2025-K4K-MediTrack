                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="36.0" prefWidth="673.0" textAlignment="CENTER">
                            <font>
                                <Font name="Poppins" size="36.0" />
                            </font>
                            <graphic>
                                <TextFlow textAlignment="CENTER">
                                    <children>
                                        <Text fill="#0c1421" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="Telusuri " wrappingWidth="140.0">
                                            <font>
                                                <Font size="36.0" />
