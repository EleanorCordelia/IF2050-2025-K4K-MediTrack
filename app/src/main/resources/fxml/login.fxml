                                <Label style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="Email" textFill="#0c1421">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="emailField" prefHeight="48.0" prefWidth="673.0" promptText="contoh@email.com" style="-fx-background-color: #F7FBFF; -fx-background-radius: 12; -fx-border-color: #D4D7E3; -fx-border-radius: 12; -fx-border-width: 1;">
                                    <font>
                                        <Font name="Poppins Regular" size="16.0" />
                                    </font>
                                </TextField>
                            </children>
                        </VBox>
                        <VBox spacing="8.0" styleClass="input-field-container">
                            <children>
                                <Label style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="Kata Sandi" textFill="#0c1421">
                                    <font>
                                        <Font size="16.0" />
                                    </font>
                                </Label>
                                <PasswordField fx:id="passwordField" prefHeight="48.0" prefWidth="673.0" promptText="Setidaknya 8 Karakter" style="-fx-background-color: #F7FBFF; -fx-background-radius: 12; -fx-border-color: #D4D7E3; -fx-border-radius: 12; -fx-border-width: 1;">
                                    <font>
                                        <Font name="Poppins Regular" size="16.0" />
                                    </font>
                                </PasswordField>
                            </children>
                        </VBox>
                        <Label style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="Lupa Kata Sandi?" textFill="#1e4ae9">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Label>
                        <Button fx:id="loginButton" mnemonicParsing="false" onAction="#handleLoginButton" prefHeight="48.0" prefWidth="673.0" style="-fx-background-color: #173EAD; -fx-background-radius: 12;" text="Masuk" textFill="WHITE">
                            <font>
                                <Font name="Poppins Bold" size="20.0" />
                            </font>
                            <VBox.margin>
                                <Insets top="16.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
                <VBox spacing="24.0">
                    <children>
