                                        <Text fill="#0c1421" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 700;" text="Kesehatanmu" wrappingWidth="230.0">
                                            <font>
                                                <Font size="36.0" />
                                            </font>
                                        </Text>
                                        <Text fill="#0c1421" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="!" wrappingWidth="10.0">
                                            <font>
                                                <Font size="36.0" />
                                            </font>
                                        </Text>
                                    </children>
                                </TextFlow>
                            </graphic>
                        </Label>
                    </children>
                </VBox>
                <Label alignment="CENTER" contentDisplay="CENTER" textAlignment="CENTER">
                    <font>
