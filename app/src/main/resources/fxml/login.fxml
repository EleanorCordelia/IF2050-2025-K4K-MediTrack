                                <Region HBox.hgrow="ALWAYS" style="-fx-border-color: #CFDFE2; -fx-border-width: 1 0 0 0;" />
                            </children>
                            <VBox.margin>
                                <Insets bottom="10.0" top="10.0" />
                            </VBox.margin>
                        </HBox>
                        <Button fx:id="loginGoogleButton" mnemonicParsing="false" onAction="#handleLoginGoogleButton" prefHeight="52.0" prefWidth="673.0" style="-fx-background-color: #F3F9FA; -fx-background-radius: 12;">
                            <graphic>
                                <HBox alignment="CENTER" spacing="16.0">
                                    <children>
                                        <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../images/google_icon.png" />
                                            </image>
                                        </ImageView>
                                        <Label style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="Masuk dengan Google" textFill="#313957">
                                            <font>
                                                <Font size="16.0" />
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                            </graphic>
                        </Button>
                    </children>
                </VBox>
                <Label alignment="CENTER" contentDisplay="CENTER" textAlignment="CENTER">
                    <font>
                        <Font name="Poppins" size="18.0" />
                    </font>
                    <graphic>
                        <TextFlow textAlignment="CENTER">
                            <children>
                                <Text fill="#313957" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="Tidak Punya Akun? ">
                                    <font>
                                        <Font size="18.0" />
                                    </font>
                                </Text>
                                <Text fx:id="registerLink" fill="#1e4ae9" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 700;" text="Registrasi" onMouseClicked="#handleRegisterLink" underline="true">
                                    <font>
                                        <Font size="18.0" />
                                    </font>
                                </Text>
                            </children>
                        </TextFlow>
                    </graphic>
                </Label>
            </children>
            <padding>
                <Insets bottom="30.0" />
            </padding>
        </VBox>
    </children>
