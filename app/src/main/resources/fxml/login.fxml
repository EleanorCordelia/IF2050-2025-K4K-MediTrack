                                <Text fill="#112f82" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 400;" text="dengan ">
                                    <font>
                                        <Font size="32.0" />
                                    </font>
                                </Text>
                                <Text fill="#112f82" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: 'Poppins'; -fx-font-weight: 600;" text="MediTrack">
                                    <font>
                                        <Font size="32.0" />
                                    </font>
                                </Text>
                            </children>
                        </TextFlow>
                    </graphic>
                </Label>
                <VBox alignment="CENTER" spacing="24.0">
