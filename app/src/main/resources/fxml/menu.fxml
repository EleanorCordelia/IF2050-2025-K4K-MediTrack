<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>

<BorderPane xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.meditrack.controller.MenuController"
            prefWidth="1920" prefHeight="1080">

    <!-- =======================
         SIDEBAR
         ======================= -->
    <left>
        <VBox fx:id="sidebar"
              alignment="TOP_LEFT"
              spacing="15"
              prefWidth="320"
              style="-fx-background-color: #FFFFFF;">
            <padding><Insets top="40" left="44" right="24" bottom="30"/></padding>

            <!-- Logo Section -->
            <HBox alignment="CENTER_LEFT" spacing="16">
                <StackPane prefWidth="56" prefHeight="56"
                           style="-fx-background-color: #1E3A8A; -fx-background-radius: 12;">
                    <ImageView fitWidth="50" fitHeight="50" preserveRatio="true">
                        <image><Image url="@/images/logo.png"/></image>
                    </ImageView>
                </StackPane>
                <Label text="MediTrack"
                       style="-fx-font-family: 'Poppins'; -fx-font-size: 24px; -fx-font-weight: 600; -fx-text-fill: #1E3A8A;"/>
            </HBox>

            <!-- Navigation Menu -->
            <VBox spacing="44" VBox.vgrow="ALWAYS" style="-fx-padding: 50 0 0 0;">
                <!-- Menu Button (Active) -->
                <Button fx:id="menuButton" text="Menu"
                        alignment="CENTER_LEFT" graphicTextGap="24" maxWidth="Infinity"
                        style="-fx-background-color: #1E40AF; -fx-text-fill: #FFFFFF; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: 600; -fx-padding: 16 24;">
                    <graphic>
                        <ImageView fitWidth="32" fitHeight="32">
                            <image><Image url="@/images/piechart.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Rekomendasi Button -->
                <Button fx:id="rekomendasiButton" text="Rekomendasi"
                        alignment="CENTER_LEFT" graphicTextGap="16" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #9CA3AF; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: normal; -fx-padding: 16;">
                    <graphic>
                        <ImageView fitWidth="20" fitHeight="20">
                            <image><Image url="@/images/graph.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Obat Button -->
                <Button fx:id="obatButton" text="Obat"
                        alignment="CENTER_LEFT" graphicTextGap="16" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #9CA3AF; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: normal; -fx-padding: 16;">
                    <graphic>
                        <ImageView fitWidth="20" fitHeight="20">
                            <image><Image url="@/images/belanja.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Laporan Kesehatan Button -->
                <Button fx:id="laporanButton" text="Laporan Kesehatan"
                        alignment="CENTER_LEFT" graphicTextGap="24" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #64748B; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: normal; -fx-padding: 16 24;">
                    <graphic>
                        <ImageView fitWidth="32" fitHeight="32">
                            <image><Image url="@/images/data.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Konsultasi Button -->
                <Button fx:id="konsultasiButton" text="Konsultasi"
                        alignment="CENTER_LEFT" graphicTextGap="24" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #64748B; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: normal; -fx-padding: 16 24;">
                    <graphic>
                        <ImageView fitWidth="32" fitHeight="32">
                            <image><Image url="@/images/konsultasi.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Jadwal Button -->
                <Button fx:id="jadwalButton" text="Jadwal"
                        alignment="CENTER_LEFT" graphicTextGap="24" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #64748B; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: normal; -fx-padding: 16 24;">
                    <graphic>
                        <ImageView fitWidth="32" fitHeight="32">
                            <image><Image url="@/images/jadwal.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Pengaturan Button -->
                <Button fx:id="pengaturanButton" text="Pengaturan"
                        alignment="CENTER_LEFT" graphicTextGap="24" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #64748B; -fx-background-radius: 16;
                               -fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: normal; -fx-padding: 16 24;">
                    <graphic>
                        <ImageView fitWidth="28" fitHeight="28">
                            <image><Image url="@/images/pengaturan.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>

                <!-- Keluar Button -->
                <Button fx:id="keluarButton" text="Keluar"
                        alignment="CENTER_LEFT" graphicTextGap="24" maxWidth="Infinity"
                        style="-fx-background-color: transparent; -fx-text-fill: #64748B;
                               -fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: normal; -fx-padding: 16 24;">
                    <graphic>
                        <ImageView fitWidth="20" fitHeight="20">
                            <image><Image url="@/images/keluar.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>
            </VBox>
        </VBox>
    </left>

    <!-- =======================
         TOP BAR
         ======================= -->
    <top>
        <HBox spacing="15" prefHeight="112"
              style="-fx-background-color: #FFFFFF; -fx-padding: 30 346 30 346;"
              alignment="CENTER_LEFT">

            <Label text="Menu"
                   style="-fx-font-size: 36px; -fx-font-weight: 600; -fx-text-fill: #1E3A8A; -fx-font-family: 'Poppins';"/>

            <Region HBox.hgrow="ALWAYS"/>

            <!-- Search Bar -->
            <StackPane style="-fx-background-color: #F9FAFB; -fx-background-radius: 32;"
                       prefWidth="513" prefHeight="56">
                <HBox alignment="CENTER_LEFT" spacing="8" style="-fx-padding: 0 32 0 24;">
                    <ImageView fitWidth="32" fitHeight="32">
                        <image><Image url="@/images/cari.png"/></image>
                    </ImageView>
                    <TextField promptText="Cari di sini..."
                               style="-fx-background-color: transparent; -fx-font-size: 18px; -fx-text-fill: #64748B;
                                      -fx-padding: 0; -fx-border-color: transparent; -fx-font-family: 'Poppins';"/>
                </HBox>
            </StackPane>

            <!-- Language Selector -->
            <HBox alignment="CENTER" spacing="14" style="-fx-padding: 8;">
                <ImageView fitWidth="24" fitHeight="24">
                    <image><Image url="@/images/indonesian.png"/></image>
                </ImageView>
                <Label text="Indo (IDN)"
                       style="-fx-font-family: 'Poppins'; -fx-font-size: 18px; -fx-font-weight: 600; -fx-text-fill: #374151;"/>
                <ImageView fitWidth="24" fitHeight="24">
                    <image><Image url="@/images/dropdown-user.png"/></image>
                </ImageView>
            </HBox>

            <!-- Notification -->
            <StackPane prefWidth="48" prefHeight="48"
                       style="-fx-background-color: #FFF7ED; -fx-background-radius: 12;">
                <ImageView fitWidth="24" fitHeight="24">
                    <image><Image url="@/images/notification.png"/></image>
                </ImageView>
                <Circle radius="6" fill="#EF4444"
                        StackPane.alignment="TOP_RIGHT">
                    <StackPane.margin>
                        <Insets top="6" right="6"/>
                    </StackPane.margin>
                </Circle>
            </StackPane>

            <!-- User Profile -->
            <HBox alignment="CENTER" spacing="12">
                <StackPane prefWidth="56" prefHeight="56">
                    <Circle radius="28" fill="#E5E7EB"/>
                    <ImageView fx:id="ivProfile" fitWidth="56" fitHeight="56" preserveRatio="true">
                        <image><Image url="@/images/user.png"/></image>
                    </ImageView>
                </StackPane>
                <Label fx:id="lblUserName" text="Carlos"
                       style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: 500; -fx-text-fill: #1E3A8A;"/>
                <Button fx:id="btnProfileDropdown"
                        style="-fx-background-color: transparent; -fx-border-color: transparent; -fx-padding: 4;">
                    <graphic>
                        <ImageView fitWidth="16" fitHeight="16">
                            <image><Image url="@/images/dropdown-user.png"/></image>
                        </ImageView>
                    </graphic>
                </Button>
            </HBox>
        </HBox>
    </top>

    <!-- =======================
         MAIN CONTENT
         ======================= -->
    <center>
        <ScrollPane fitToWidth="true" style="-fx-background-color: #F9FAFB;">
            <VBox spacing="40" style="-fx-padding: 0 346 100 346;">

                <!-- Hero Banner -->
                <StackPane prefHeight="302" style="-fx-background-color: #DBEAFE; -fx-background-radius: 0;">
                    <ImageView fitWidth="1577" fitHeight="302" preserveRatio="false">
                        <image><Image url="@/images/hero-banner.png"/></image>
                    </ImageView>

                    <!-- Hero Content -->
                    <VBox alignment="CENTER" spacing="20">
                        <VBox.margin><Insets top="50"/></VBox.margin>

                        <!-- Welcome Text -->
                        <HBox alignment="CENTER">
                            <Label style="-fx-font-family: 'Poppins'; -fx-font-size: 48px; -fx-font-weight: normal; -fx-text-fill: #1E3A8A;">
                                <text>Selamat Datang, </text>
                            </Label>
                            <Label text="Carlos" style="-fx-font-family: 'Poppins'; -fx-font-size: 48px; -fx-font-weight: bold; -fx-text-fill: #1E3A8A;"/>
                            <Label text="!" style="-fx-font-family: 'Poppins'; -fx-font-size: 48px; -fx-font-weight: normal; -fx-text-fill: #1E3A8A;"/>
                        </HBox>

                        <!-- Subtitle -->
                        <Label alignment="CENTER" style="-fx-font-family: 'Poppins'; -fx-font-size: 24px; -fx-font-weight: normal; -fx-text-fill: #1E3A8A;">
                            <text>Hidup Sehat dengan </text>
                        </Label>
                        <Label text="MediTrack" style="-fx-font-family: 'Poppins'; -fx-font-size: 24px; -fx-font-weight: 600; -fx-text-fill: #1E3A8A;"/>

                        <!-- Description -->
                        <Label alignment="CENTER" wrapText="true" maxWidth="1491"
                               style="-fx-font-family: 'Poppins'; -fx-font-size: 24px; -fx-font-weight: normal; -fx-text-fill: #1E3A8A;"
                               text="Mulai perjalanan kesehatanmu bersama MediTrack dengan fitur lengkap yang membantu memantau, mengelola, dan meningkatkan kualitas hidup setiap hari."/>
                    </VBox>
                </StackPane>

                <!-- Dashboard Cards Section -->
                <HBox spacing="40" alignment="TOP_LEFT">

                    <!-- Data Biometrik Card -->
                    <VBox spacing="24" prefWidth="877" style="-fx-background-color: #FFFFFF; -fx-background-radius: 20; -fx-effect: dropshadow(gaussian, rgba(238,238,238,0.5), 20, 0, 0, 4); -fx-padding: 32;">

                        <!-- Card Header -->
                        <HBox alignment="CENTER_LEFT">
                            <VBox spacing="8">
                                <Label text="Data Biometrik"
                                       style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: 600; -fx-text-fill: #1E3A8A;"/>
                                <Label style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-text-fill: #64748B;">
                                    <text>Kondisi Tubuh</text>
                                </Label>
                                <Label text=" Saat Ini..." style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: normal; -fx-text-fill: #64748B;"/>
                            </VBox>
                            <Region HBox.hgrow="ALWAYS"/>
                            <Button text="Lihat Selengkapnya"
                                    style="-fx-background-color: transparent; -fx-border-color: #CBD5E1; -fx-border-radius: 8; -fx-text-fill: #0F172A; -fx-font-family: 'Poppins'; -fx-font-size: 14px; -fx-font-weight: 500; -fx-padding: 12 16;">
                                <graphic>
                                    <ImageView fitWidth="14" fitHeight="14">
                                        <image><Image url="@/images/arrow-right.png"/></image>
                                    </ImageView>
                                </graphic>
                            </Button>
                        </HBox>

                        <!-- Biometric Data Grid -->
                        <GridPane hgap="40" vgap="0">
                            <columnConstraints>
                                <ColumnConstraints percentWidth="25"/>
                                <ColumnConstraints percentWidth="25"/>
                                <ColumnConstraints percentWidth="25"/>
                                <ColumnConstraints percentWidth="25"/>
                            </columnConstraints>

                            <!-- Blood Pressure -->
                            <VBox GridPane.columnIndex="0" spacing="16" alignment="CENTER"
                                  style="-fx-background-color: #FEF2F2; -fx-background-radius: 16; -fx-padding: 24;">
                                <StackPane prefWidth="40" prefHeight="40" style="-fx-background-color: #FB7185; -fx-background-radius: 20;">
                                    <ImageView fitWidth="20" fitHeight="20">
                                        <image><Image url="@/images/blood-pressure.png"/></image>
                                    </ImageView>
                                </StackPane>
                                <Label text="120/80" style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: bold; -fx-text-fill: #1E3A8A;"/>
                                <VBox alignment="CENTER" spacing="4">
                                    <Label text="mmHg" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #64748B;"/>
                                    <Label text="Tekanan Darah" style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: 500; -fx-text-fill: #000000;"/>
                                </VBox>
                            </VBox>

                            <!-- Heart Rate -->
                            <VBox GridPane.columnIndex="1" spacing="16" alignment="CENTER"
                                  style="-fx-background-color: #FFF7ED; -fx-background-radius: 16; -fx-padding: 24;">
                                <StackPane prefWidth="40" prefHeight="40" style="-fx-background-color: #FB7185; -fx-background-radius: 20;">
                                    <ImageView fitWidth="20" fitHeight="20">
                                        <image><Image url="@/images/detakhati.png"/></image>
                                    </ImageView>
                                </StackPane>
                                <Label text="110" style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: bold; -fx-text-fill: #1E3A8A;"/>
                                <VBox alignment="CENTER" spacing="4">
                                    <Label text="BPM" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #64748B;"/>
                                    <Label text="Detak Jantung" style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: 500; -fx-text-fill: #000000;"/>
                                </VBox>
                            </VBox>

                            <!-- Body Temperature -->
                            <VBox GridPane.columnIndex="2" spacing="16" alignment="CENTER"
                                  style="-fx-background-color: #F0FDF4; -fx-background-radius: 16; -fx-padding: 24;">
                                <StackPane prefWidth="40" prefHeight="40" style="-fx-background-color: #22C55E; -fx-background-radius: 20;">
                                    <ImageView fitWidth="20" fitHeight="20">
                                        <image><Image url="@/images/thermometer.png"/></image>
                                    </ImageView>
                                </StackPane>
                                <Label text="36.5" style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: bold; -fx-text-fill: #1E3A8A;"/>
                                <VBox alignment="CENTER" spacing="4">
                                    <Label text="Â°C" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #64748B;"/>
                                    <Label text="Suhu Tubuh" style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: 500; -fx-text-fill: #000000;"/>
                                </VBox>
                            </VBox>

                            <!-- Stress Level -->
                            <VBox GridPane.columnIndex="3" spacing="16" alignment="CENTER"
                                  style="-fx-background-color: #FAF5FF; -fx-background-radius: 16; -fx-padding: 24;">
                                <StackPane prefWidth="40" prefHeight="40" style="-fx-background-color: #A855F7; -fx-background-radius: 20;">
                                    <ImageView fitWidth="20" fitHeight="20">
                                        <image><Image url="@/images/otak.png"/></image>
                                    </ImageView>
                                </StackPane>
                                <Label text="Tinggi" style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: bold; -fx-text-fill: #1E3A8A;"/>
                                <VBox alignment="CENTER" spacing="4">
                                    <Label text="Rendah-Sedang-Tinggi" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #64748B;"/>
                                    <Label text="Tingkat Stress" style="-fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: 500; -fx-text-fill: #000000;"/>
                                </VBox>
                            </VBox>
                        </GridPane>
                    </VBox>

                    <!-- Data Kesehatan Fisik Card -->
                    <VBox spacing="24" prefWidth="591" style="-fx-background-color: #FFFFFF; -fx-background-radius: 20; -fx-effect: dropshadow(gaussian, rgba(238,238,238,0.5), 20, 0, 0, 4); -fx-padding: 32;">

                        <!-- Card Header -->
                        <HBox alignment="CENTER_LEFT">
                            <Label text="Data Kesehatan Fisik"
                                   style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: 600; -fx-text-fill: #1E3A8A;"/>
                            <Region HBox.hgrow="ALWAYS"/>
                            <Button text="Lihat Selengkapnya"
                                    style="-fx-background-color: transparent; -fx-border-color: #CBD5E1; -fx-border-radius: 8; -fx-text-fill: #0F172A; -fx-font-family: 'Poppins'; -fx-font-size: 14px; -fx-font-weight: 500; -fx-padding: 12 16;">
                                <graphic>
                                    <ImageView fitWidth="14" fitHeight="14">
                                        <image><Image url="@/images/arrow-right.png"/></image>
                                    </ImageView>
                                </graphic>
                            </Button>
                        </HBox>

                        <!-- Chart Area -->
                        <StackPane prefHeight="192" style="-fx-background-color: #FAFAFA;">
                            <LineChart fx:id="healthChart" prefHeight="192" createSymbols="false">
                                <xAxis>
                                    <CategoryAxis fx:id="xAxis"/>
                                </xAxis>
                                <yAxis>
                                    <NumberAxis fx:id="yAxis" autoRanging="false" lowerBound="0" upperBound="400" tickUnit="100"/>
                                </yAxis>
                            </LineChart>
                        </StackPane>

                        <!-- Chart Legend -->
                        <HBox spacing="32" alignment="CENTER">
                            <HBox spacing="8" alignment="CENTER_LEFT">
                                <Circle radius="6" fill="#A855F7"/>
                                <Label text="Durasi Tidur" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #6B7280;"/>
                            </HBox>
                            <HBox spacing="8" alignment="CENTER_LEFT">
                                <Circle radius="6" fill="#EF4444"/>
                                <Label text="Durasi Olahraga" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #6B7280;"/>
                            </HBox>
                            <HBox spacing="8" alignment="CENTER_LEFT">
                                <Circle radius="6" fill="#22C55E"/>
                                <Label text="Jumlah Langkah" style="-fx-font-family: 'Poppins'; -fx-font-size: 12px; -fx-font-weight: 500; -fx-text-fill: #6B7280;"/>
                            </HBox>
                        </HBox>
                    </VBox>
                </HBox>

                <!-- CTA Section -->
                <StackPane prefHeight="712" style="-fx-background-color: linear-gradient(135deg, #1E40AF 0%, #3B82F6 100%); -fx-background-radius: 20;">
                    <ImageView fitWidth="1515" fitHeight="712" preserveRatio="false">
                        <image><Image url="@/images/cta-background.png"/></image>
                    </ImageView>

                    <VBox alignment="CENTER" spacing="28">
                        <!-- Icon -->
                        <StackPane prefWidth="48" prefHeight="48" style="-fx-background-color: #FFFFFF; -fx-background-radius: 16;">
                            <!-- Medical icon placeholder -->
                        </StackPane>

                        <!-- CTA Text -->
                        <Label text="Buat Rekomendasi Obat dan Suplemen?"
                               style="-fx-font-family: 'Poppins'; -fx-font-size: 36px; -fx-font-weight: 600; -fx-text-fill: #FFFFFF;"
                               alignment="CENTER"/>

                        <!-- Subtitle -->
                        <Label text="Klik Sehatmu Dimulai di Sini"
                               style="-fx-font-family: 'Poppins'; -fx-font-size: 20px; -fx-font-weight: 500; -fx-text-fill: rgba(255,255,255,0.8);"
                               alignment="CENTER"/>

                        <!-- Generate Button -->
                        <Button fx:id="btnGenerate" text="Generate" onAction="#handleGenerate"
                                style="-fx-background-color: #FFFFFF; -fx-text-fill: #1E40AF; -fx-background-radius: 8; -fx-font-family: 'Poppins'; -fx-font-size: 16px; -fx-font-weight: 600; -fx-padding: 8 40;"/>
                    </VBox>
                </StackPane>

                <!-- Schedule Section -->
                <VBox spacing="32" style="-fx-background-color: #FFFFFF; -fx-background-radius: 20; -fx-effect: dropshadow(gaussian, rgba(238,238,238,0.5), 20, 0, 0, 4); -fx-padding: 40;">

                    <!-- Section Header -->
                    <HBox alignment="CENTER_LEFT">
                        <VBox spacing="12">
                            <HBox spacing="16" alignment="CENTER_LEFT">
                                <StackPane prefWidth="48" prefHeight="48" style="-fx-background-color: #6D28D9; -fx-background-radius: 16;">
                                    <!-- Schedule icon placeholder -->
                                </StackPane>
                                <VBox>
                                    <Label text="Kenali Dirimu!" style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: bold; -fx-text-fill: #1E3A8A;"/>
                                    <Label text="Jadwal Hari ini" style="-fx-font-family: 'Poppins'; -fx-font-size: 30px; -fx-font-weight: 600; -fx-text-fill: #1E3A8A;"/>
                                </VBox>
                            </HBox>
                        </VBox>
                        <Region HBox.hgrow="ALWAYS"/>
                        <Button text="Lihat Selengkapnya"
                                style="-fx-background-color: transparent; -fx-border-color: #CBD5E1; -fx-border-radius: 8; -fx-text-fill: #0F172A; -fx-font-family: 'Poppins'; -fx-font-size: 14px; -fx-font-weight: 500; -fx-padding: 12 16;">
                            <graphic>
                                <ImageView fitWidth="14" fitHeight="14">
                                    <image><Image url="@/images/arrow-right.png"/></image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </HBox>

                    <!-- Schedule Items -->
                    <VBox spacing="24">

                        <!-- Morning Exercise -->
                        <HBox spacing="24" alignment="CENTER_LEFT"
                              style="-fx-background-color: #F0FDF4; -fx-background-radius: 10; -fx-padding: 24;">
